{
  "expo": {
    "name": "FoodNinja",
    "slug": "foodninja",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.muhammadyounis.foodninja"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.muhammadyounis.foodninja"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "f98b8020-1fa1-42c0-91ad-51b1cd4daee5"
      }
    },
    "owner": "muhammadyounis",
    "updates": {
      "url": "https://u.expo.dev/f98b8020-1fa1-42c0-91ad-51b1cd4daee5"
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
